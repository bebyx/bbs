<%= form_with model: @board, local: true do |form| %>

<%= render 'shared/errors', obj: @board %>

  <p>
    <%= form.label :name %><br/>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :description %><br/>
    <%= form.text_area :description %>
  </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>
